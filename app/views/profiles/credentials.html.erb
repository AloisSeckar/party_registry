<%
page_title "Přihlašovací údaje"
#breadcrumb(:branch, @branch)
%>

<%= render('profiles/nav') %>

<%= simple_form_for current_person, url: wizard_path, as: 'person' do |f| %>
<%= f.error_notification %>
<%= f.input :username %>
<%= f.input :password %>
<%= f.input :password_confirmation %>
<%= f.button :submit, value: "Uložit", class: 'btn btn-success' %>
<% end %>
